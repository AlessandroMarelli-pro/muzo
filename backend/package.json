{
  "name": "muzo-backend",
  "version": "1.0.0",
  "description": "Muzo AI-powered music library organization backend API",
  "author": "Alessandro Marelli",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "test:contract": "vitest run tests/contract",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "redis:up": "docker-compose -f docker-compose.dev.yml up -d redis-dev",
    "redis:down": "docker-compose -f docker-compose.dev.yml down",
    "redis:logs": "docker-compose -f docker-compose.dev.yml logs -f redis-dev",
    "redis:cli": "docker exec -it muzo-redis-dev redis-cli",
    "redis:flush": "docker exec -it muzo-redis-dev redis-cli FLUSHALL",
    "dev:setup": "npm run redis:up && npm run prisma:generate",
    "dev:cleanup": "npm run redis:down",
    "bull-board": "node ./bull-board.js"
  },
  "keywords": [
    "music",
    "ai",
    "library",
    "organization",
    "nestjs",
    "graphql"
  ],
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.13.0",
    "@bull-board/nestjs": "^6.14.0",
    "@elastic/elasticsearch": "^9.1.1",
    "@libsql/client": "^0.15.15",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/elasticsearch": "^11.1.0",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/platform-ws": "^11.1.6",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.16.2",
    "@types/axios": "^0.9.36",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node-wav": "^0.0.4",
    "axios": "^1.12.2",
    "bullmq": "^5.58.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fluent-ffmpeg": "^2.1.3",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^3.0.0",
    "ioredis": "^5.7.0",
    "node-wav": "^0.0.2",
    "prisma": "^6.16.2",
    "puppeteer": "^24.22.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/puppeteer": "^5.4.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "graphql-request": "^7.2.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
